/* pages/profile/profile.wxss */
.profile-header{
    width:100%;
    height:260rpx;
    background:#FFAEBD;
    display: flex;
    flex-direction: column;
     justify-content: center; 
     align-items: center; 
     font-size:16px;
}
.profile-header image{
    width:160rpx;
    height:160rpx;
    border-radius: 50%;
    margin:20rpx 0;
}
.profile-body{
    width:100%;
    display: flex;
    font-size:16px;
}
.profile-body .item{
    width:33.33%;
    height:160rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.profile-body .item text{
    margin:10rpx 0rpx;
}